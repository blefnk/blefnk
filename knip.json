{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"project": ["**/*.{ts,js}"],
	"entry": ["*.{config,cfg}.{ts,js}"],
	"ignore": [
		"node_modules",
		"dist-jsr",
		"dist-npm",
		"dist-libs",
		"src/lib/utils.ts",
